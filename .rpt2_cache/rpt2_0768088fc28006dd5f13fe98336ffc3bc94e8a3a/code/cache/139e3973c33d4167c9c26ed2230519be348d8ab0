{"code":"import React from 'react';\r\nimport { Link } from 'react-router-dom';\r\nimport styled from 'styled-components';\r\nimport { gray300 } from '../Colors';\r\nimport { body2Paragraph } from '../TextStyles';\r\nimport Typography from '../Typography';\r\nconst Header = styled.div `\n  margin-bottom: 16px;\n  a {\n    display: inline-block;\n    position: relative;\n    padding-right: 24px;\n  }\n`;\r\nconst HeaderTitle = styled(Typography) `\n  display: inline-block;\n`;\r\nconst SubTitle = styled.p `\n  ${body2Paragraph}\n  margin-top: 4px;\n`;\r\nconst HeaderIcon = styled.span `\n  position: absolute;\n  top: 50%;\n  transform: translateY(-50%);\n  svg {\n    display: block;\n  }\n`;\r\nexport default ({ title, subTitle, typography, to, href, target, ...restProps }) => {\r\n    const options = {};\r\n    if (target === '_blank') {\r\n        options.rel = 'noopener noreferrer';\r\n    }\r\n    const sectionHeaderTitle = (React.createElement(HeaderTitle, Object.assign({ md: typography || 'Subtitle1' }, restProps), title));\r\n    const sectionHeaderIcon = (React.createElement(HeaderIcon, null,\r\n        React.createElement(\"svg\", { width: 24, height: 24, viewBox: \"0 0 24px 24px\" },\r\n            React.createElement(\"path\", { fill: gray300, fillRule: \"evenodd\", d: \"M8.5 18.5l6-6.5-6-6.5L10 4l7.5 8-7.5 8z\" }))));\r\n    if (to) {\r\n        return (React.createElement(Header, null,\r\n            React.createElement(Link, Object.assign({ to: to, target: target }, options),\r\n                sectionHeaderTitle,\r\n                sectionHeaderIcon),\r\n            subTitle && React.createElement(SubTitle, null, subTitle)));\r\n    }\r\n    if (href) {\r\n        return (React.createElement(Header, null,\r\n            React.createElement(\"a\", Object.assign({ href: href, target: target }, options),\r\n                sectionHeaderTitle,\r\n                sectionHeaderIcon),\r\n            subTitle && React.createElement(SubTitle, null, subTitle)));\r\n    }\r\n    return (React.createElement(Header, null,\r\n        sectionHeaderTitle,\r\n        subTitle && React.createElement(SubTitle, null, subTitle)));\r\n};\r\n","dts":{"name":"/Users/baehyeonseung/pedaling/class101-ui/Section/Header.d.ts","writeByteOrderMark":false,"text":"/// <reference types=\"react\" />\r\nimport { Typo } from '../Typography';\r\ninterface Props {\r\n    title?: string;\r\n    subTitle?: string;\r\n    typography?: Typo;\r\n    to?: string;\r\n    href?: string;\r\n    target?: string;\r\n}\r\ndeclare const _default: ({ title, subTitle, typography, to, href, target, ...restProps }: Props) => JSX.Element;\r\nexport default _default;\r\n"}}
